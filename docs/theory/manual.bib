issn = {0269-2139},
journal = {Protein Eng.},
keywords = {Coarse Grained,MJ-parameter,hydrophobic energy,inter-residue contact energy,native-like compact denatured state,protein,protein folding},
mendeley-tags = {Coarse Grained,MJ-parameter},
number = {10},
