doi = {10.1103/PhysRevE.68.061408},
file = {:home/max/academia/paper/pdfs/Physical Review E/2003{\_}Urbina-Villalba, Garc{\'{i}}a-Sucre, Toro-Mendoza{\_}Average hydrodynamic correction for the Brownian dynamics calculation of flocculation.pdf:pdf},
