@article{Ermak1978,
abstract = {A method for simulating the Brownian dynamics of N particles with$\backslash$nthe inclusion of hydrodynamic interactions is described. The particles$\backslash$nmay also be subject to the usual interparticle or external forces$\backslash$n(e.g., electrostatic) which have been included in previous methods$\backslash$nfor simulating Brownian dynamics of particles in the absence of hydrodynamic$\backslash$ninteractions. The present method is derived from the Langevin equations$\backslash$nfor the N particle assembly, and the results are shown to be consistent$\backslash$nwith the corresponding Fokker–Planck results. Sample calculations$\backslash$non small systems illustrate the importance of including hydrodynamic$\backslash$ninteractions in Brownian dynamics simulations. The method should$\backslash$nbe useful for simulation studies of diffusion limited reactions,$\backslash$npolymer dynamics, protein folding, particle coagulation, and other$\backslash$nphenomena in solution. The Journal of Chemical Physics is copyrighted$\backslash$nby The American Institute of Physics.},
