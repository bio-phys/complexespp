keywords = {Cellulosome,Coarse-grained simulations,Conformational ensemble,Intrinsically disordered proteins,Multi-domain proteins,Small angle X-ray scattering},
number = {1},
pages = {68--75},
pmid = {26008791},
title = {{Large conformational fluctuations of the multi-domain xylanase Z of Clostridium thermocellum}},
volume = {191},
year = {2015}
}
