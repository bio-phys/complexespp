file = {:home/max/academia/paper/pdfs/The Journal of Chemical Physics/2013{\_}Chevrot, Hinsen, Kneller{\_}Model-free simulation approach to molecular diffusion tensors.pdf:pdf},
isbn = {doi:10.1063/1.4823996},
issn = {1089-7690},
journal = {J. Chem. Phys.},
number = {15},
