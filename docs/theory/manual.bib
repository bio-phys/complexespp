journal = {J. Phys. le Radium},
number = {1},
pages = {1--11},
pmid = {10094422},
title = {{Mouvement Brownien d'un ellipsoide (II). Rotation libre et d{\'{e}}polarisation des fluorescences. Translation et diffusion de mol{\'{e}}cules ellipsoidales}},
