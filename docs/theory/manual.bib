journal = {J. Phys. Chem. Ref. Data},
keywords = {EQUATIONS OF STATE,STEAM,SURFACE TENSION,THERMODYNAMIC PROPERTIES,VISCOSITY,WATER,WATER VAPOR,equation of state,surface tension,thermal conductivity,thermodynamic properties,viscosity,water},
month = {jan},
