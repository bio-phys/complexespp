@article{Wolf2012,
abstract = {In the present work, we provide a dielectric study on two differently$\backslash$nconcentrated aqueous lysozyme solutions in the frequency range from$\backslash$n1{\&}{\#}xa0;MHz to 40{\&}{\#}xa0;GHz and for temperatures from 275 to 330{\&}{\#}xa0;K.$\backslash$nWe analyze the three dispersion regions, commonly found in protein$\backslash$nsolutions, usually termed $\beta$-, $\gamma$-, and $\delta$-relaxations. The $\beta$-relaxation,$\backslash$noccurring in the frequency range around 10{\&}{\#}xa0;MHz and the $\gamma$-relaxation$\backslash$naround 20{\&}{\#}xa0;GHz (at room temperature) can be attributed to the$\backslash$nrotation of the polar protein molecules in their aqueous medium and$\backslash$nthe reorientational motion of the free water molecules, respectively.$\backslash$nThe nature of the $\delta$-relaxation, which is often ascribed to the motion$\backslash$nof bound water molecules, is not yet fully understood. Here we provide$\backslash$ndata on the temperature dependence of the relaxation times and relaxation$\backslash$nstrengths of all three detected processes and on the dc conductivity$\backslash$narising from ionic charge transport. The temperature dependences$\backslash$nof the $\beta$- and $\gamma$-relaxations are closely correlated. We found a$\backslash$nsignificant temperature dependence of the dipole moment of the protein,$\backslash$nindicating conformational changes. Moreover we find a breakdown of$\backslash$nthe Debye–Stokes–Einstein relation in this protein solution,$\backslash$ni.e., the dc conductivity is not completely governed by the mobility$\backslash$nof the solvent molecules. Instead it seems that the dc conductivity$\backslash$nis closely connected to the hydration shell dynamics.},
author = {Wolf, M. and Gulich, R. and Lunkenheimer, P. and Loidl, A.},
doi = {10.1016/j.bbapap.2012.02.008},
file = {:home/max/academia/paper/pdfs/Biochimica et Biophysica Acta (BBA)-Proteins and Proteomics/2012{\_}Wolf et al.{\_}Relaxation dynamics of a protein solution investigated by dielectric spectroscopy.pdf:pdf},
isbn = {1570-9639},
issn = {15709639},
