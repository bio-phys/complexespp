doi = {10.1021/jp301140u},
file = {:home/max/academia/paper/pdfs/Journal of Physical Chemistry B/2012{\_}Chaudhri et al.{\_}Coarse-grained modeling of the self-association of therapeutic monoclonal antibodies(2).pdf:pdf},
