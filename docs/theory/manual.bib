eprint = {z0024},
file = {:home/max/academia/paper/pdfs/Journal of the American Chemical Society/1995{\_}Cornell et al.{\_}A Second Generation Force Field for the Simulation of Proteins, Nucleic Acids, and Organic Molecules.pdf:pdf},
isbn = {0120261464},
issn = {0002-7863},
journal = {J. Am. Chem. Soc.},
